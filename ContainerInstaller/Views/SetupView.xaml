<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ContainerInstaller.Views"
             xmlns:ViewModels="clr-namespace:ContainerInstaller.ViewModels" x:Class="ContainerInstaller.Views.SetupView"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Style="{StaticResource GeneralPageStyle}">
    <UserControl.DataContext>
        <ViewModels:SetupViewModel/>
    </UserControl.DataContext>
    <Grid>
        <!-- Setup form wich will be used to make host prepared for Container installations -->
        <StackPanel>
            
            <!-- Navigating to ContainerInstallerView when done setup environment -->
            <Button Click="Button_Click" >Setup</Button>
        </StackPanel>
    </Grid>
</UserControl>
